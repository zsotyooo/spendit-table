{
  "name": "@spendit/table",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/spendit-table.umd.js",
  "module": "./dist/spendit-table.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/spendit-table.es.js",
      "require": "./dist/spendit-table.umd.js"
    }
  },
  "scripts": {
    "dev": "run-s build && run-p vite:dev storybook:dev",
    "build": "run-s typescript:build vite:build storybook:build",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,json}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "storybook:dev": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "vite:dev": "vite build --watch",
    "vite:build": "vite build",
    "typescript:build": "tsc",
    "preinstall": "yarn build",
    "test:dev": "vitest dev",
    "test": "vitest run"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@faker-js/faker": "^7.6.0",
    "@spendit/eslint-config-custom": "*",
    "@spendit/typescript": "*",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.3.0",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^3.0.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.32.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.27.3"
  }
}
